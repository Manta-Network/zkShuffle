{
  "name": "@zkshuffle/jssdk",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist.esm/index.js",
  "types": "src/index.ts",
  "publishConfig": {
    "types": "dist/index.d.ts"
  },
  "license": "MIT",
  "description": "A Typescript SDK for NodeJS and frontend ZK ability integration",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:esm && npm run build:commonjs",
    "clear": "rm -rf ./dist/ ./dist.esm/",
    "build:commonjs": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "pub2npmjs": "npm publish --access=public",
    "lint:check": "eslint -c '../../.eslintrc' './**/*.ts'",
    "lint:fix": "eslint -c '../../.eslintrc' --fix './**/*.ts'",
    "prettier:check": "prettier --check './**/*.ts'",
    "prettier:fix": "prettier --write './**/*.ts'"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@typechain/hardhat": "^6.1.4",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "ethereum-waffle": "^3.4.4",
    "hardhat": "^2.12.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@typechain/ethers-v5": "^10.1.1",
    "@zkshuffle/proof": "1.0.0",
    "axios": "^1.4.0",
    "circomlibjs": "^0.1.7",
    "ethers": "^5.7.2",
    "patch-package": "^7.0.0"
  },
  "files": [
    "src",
    "dist",
    "dist.esm"
  ]
}
